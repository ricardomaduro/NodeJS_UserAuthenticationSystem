extends layout

block content
  h1 Login Area
  p Please login bellow
  form(method='post',action='/users/login',entType='multipart/form-data')
    .form-group
        label Username
        input.form-control(name='username',type='text')
    .form-group
        label Password
        input.form-control(name='password',type='password')
    
    input.btn.btn-default(name='submit',type='submit',value='Login')
